@using TravelTripProje.Models.Sınıflar
@model List<Blog>
<div class="padding1">
    <h4>En beğendiğim yerler</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
            <div class="bot-img">
               <a href="/Blog/BlogDetay/@x.ID"> <img src="@x.BlogImage" src="@x.BlogImage" style="width:120px;height:65px;" alt=""  /> </a>
            </div>
            <div class="bot-desc">
                <p>@x.Aciklama.Substring(0,60)...</p>
            </div>
            <div class="clear"></div>        
        }
    </div>
    @Html.Action("Partial5","Default")
</div>